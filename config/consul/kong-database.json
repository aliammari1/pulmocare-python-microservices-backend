    "service": {
        "name": "kong-database",
        "tags": [
            "postgres",
            "database"
        ],
        "port": 5432,
        "check": {
            "id": "postgres",
            "name": "PostgreSQL TCP on port 5432",
            "tcp": "kong-database:5432",
            "interval": "10s",
            "timeout": "5s"
        }
    }
}